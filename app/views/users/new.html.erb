<%= image_tag @user.image.url(:thumb) %>
<%= form_for @user , html: { multipart: true } do |f| %> 
<%= f.label :image%><br /> <%= f.file_field :image %> <br />
<%= f.label :title%><br /> <%= f.text_field :name %> <br />
<%= f.label :content %> <br /><%= f.text_field :age %><br />
<%= f.submit 'save '%>
<% end %>